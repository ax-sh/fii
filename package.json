{
  "name": "fii",
  "version": "0.73.0",
  "description": "fii CLI",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "fii": "bin/fii"
  },
  "scripts": {
    "release": "dotenv -v GIT_MERGE_AUTOEDIT=no release-it -- minor --ci",
    "released": "dotenv -v GIT_MERGE_AUTOEDIT=no release-it -- minor --no-git.push --no-git.requireUpstream --ci",
    "clean-build": "rimraf -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles ./src/templates/* ./build/templates",
    "build": "bun clean-build && bun compile && bun copy-templates",
    "prepublishOnly": "yarn build",
    "format": "eslint --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "upp": "bunx npm-check-updates -i",
    "pretty": "pretty-quick",
    "prettier:fix": "prettier . --write",
    "db:gen": "bunx drizzle-kit generate --dialect pglite --schema ./src/lib/db/schema.ts",
    "test": "bun test",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "files": [
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@ax-sh/browser-cookies": "^0.13.0",
    "@faker-js/faker": "^8.4.1",
    "axios": "^1.7.2",
    "axios-cookiejar-support": "^5.0.2",
    "dayjs": "^1.11.11",
    "drizzle-orm": "^0.31.2",
    "eslint-plugin-sonarjs": "^1.0.3",
    "execa": "^9.2.0",
    "fs-jetpack": "^5.1.0",
    "gluegun": "latest",
    "jsonc-parser": "^3.2.1",
    "langchain": "^0.2.5",
    "playwright": "^1.44.1",
    "playwright-extra": "^4.3.6",
    "puppeteer": "^22.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "pythonia": "^1.2.1",
    "rimraf": "^5.0.7",
    "tough-cookie": "^4.1.4",
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "globals": "^15.5.0",
    "@eslint/js": "^9.5.0",
    "@playwright/test": "^1.44.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "^1.1.4",
    "@types/node": "^20.14.2",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.22.7",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "husky": "^9.0.11",
    "oxlint": "^0.4.4",
    "prettier": "^3.3.2",
    "pretty-quick": "^4.0.0",
    "release-it": "^17.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.5",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.13.0",
    "xo": "^0.58.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
