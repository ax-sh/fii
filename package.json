{
  "name": "fii",
  "version": "0.92.0",
  "description": "fii CLI",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "fii": "bin/fii"
  },
  "scripts": {
    "release": "dotenv -v GIT_MERGE_AUTOEDIT=no release-it -- minor --ci",
    "release-local-ci": "dotenv -v GIT_MERGE_AUTOEDIT=no release-it -- minor --no-git.push --no-git.requireUpstream --ci",
    "clean-build": "rimraf -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles ./src/templates/* ./build/templates",
    "build": "bun clean-build && bun compile && bun copy-templates",
    "prepublishOnly": "nr build",
    "format:old": "eslint --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "format": "nr biome check --write",
    "upp:old": "bunx npm-check-updates -i",
    "upp": "bun update --latest",
    "pretty": "pretty-quick",
    "prettier:fix": "prettier . --write",
    "db:gen": "bunx drizzle-kit generate --dialect pglite --schema ./src/lib/db/schema.ts",
    "test": "vitest run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:types": "tsc --noEmit"
  },
  "files": ["build", "LICENSE", "readme.md", "docs", "bin"],
  "license": "MIT",
  "dependencies": {
    "@ax-sh/browser-cookies": "^0.13.0",
    "@cspell/eslint-plugin": "^8.14.4",
    "@faker-js/faker": "^9.0.3",
    "axios": "^1.7.7",
    "axios-cookiejar-support": "^5.0.3",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.33.0",
    "eslint-plugin-compat": "^6.0.1",
    "eslint-plugin-sonarjs": "^2.0.3",
    "execa": "^9.4.0",
    "fs-jetpack": "^5.1.0",
    "git-cliff": "^2.6.1",
    "gluegun": "^5.2.0",
    "jsonc-parser": "^3.3.1",
    "kolorist": "^1.8.0",
    "langchain": "^0.3.2",
    "playwright": "^1.47.2",
    "playwright-extra": "^4.3.6",
    "puppeteer": "^23.5.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "pythonia": "^1.2.1",
    "rimraf": "^6.0.1",
    "simple-git": "^3.27.0",
    "tough-cookie": "^5.0.0",
    "ts-morph": "^24.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.47.2",
    "@release-it/conventional-changelog": "^8.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "^1.1.10",
    "@types/node": "^22.7.4",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "oxlint": "^0.9.9",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "release-it": "^17.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.2",
    "xo": "^0.59.3"
  }
}
