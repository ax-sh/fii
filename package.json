{
  "name": "fii",
  "version": "0.107.0",
  "description": "fii CLI",
  "private": true,
  "types": "build/types/types.d.ts",
  "bin": {
    "fii": "bin/fii"
  },
  "scripts": {
    "release": "dotenv -v GIT_MERGE_AUTOEDIT=no release-it -- minor --ci",
    "release-local-ci": "dotenv -v GIT_MERGE_AUTOEDIT=no release-it -- minor --no-git.push --no-git.requireUpstream --ci",
    "clean-build": "rimraf -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles ./src/templates/* ./build/templates",
    "build": "bun clean-build && bun compile && bun copy-templates",
    "prepublishOnly": "nr build",
    "format:old": "eslint --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "format": "nr biome check --write",
    "upp:old": "bunx npm-check-updates -i",
    "upp": "bun update --latest",
    "pretty": "pretty-quick",
    "prettier:fix": "prettier . --write",
    "db:gen": "bunx drizzle-kit generate --dialect pglite --schema ./src/lib/db/schema.ts",
    "test": "vitest run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:types": "tsc --noEmit",
    "changelog:latest": "nr git-cliff -l"
  },
  "files": ["build", "LICENSE", "readme.md", "docs", "bin"],
  "license": "MIT",
  "dependencies": {
    "@cspell/eslint-plugin": "^8.17.2",
    "@faker-js/faker": "^9.4.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.38.4",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-sonarjs": "^3.0.1",
    "execa": "^9.5.2",
    "fs-jetpack": "^5.1.0",
    "git-cliff": "^2.8.0",
    "gluegun": "^5.2.0",
    "jsonc-parser": "^3.3.1",
    "kolorist": "^1.8.0",
    "langchain": "^0.3.13",
    "npm-registry-client": "^8.6.0",
    "octokit": "^4.1.0",
    "playwright": "^1.50.0",
    "playwright-extra": "^4.3.6",
    "puppeteer": "^24.1.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "pythonia": "^1.2.2",
    "rimraf": "^6.0.1",
    "simple-git": "^3.27.0",
    "tough-cookie": "^5.1.0",
    "ts-morph": "^25.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.50.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/bun": "^1.2.0",
    "@types/node": "^22.10.10",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "oxlint": "^0.15.8",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "release-it": "^18.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^3.0.4",
    "xo": "^0.60.0"
  }
}
